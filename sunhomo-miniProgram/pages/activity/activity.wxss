.header {
  padding: 80rpx;
  line-height: 1;
}
.title {
  font-size: 52rpx;
}
.menu-list {
  display: flex;
  flex-direction: column;
  background-color: #fbf9fe;
  margin-top: 10rpx;
}
.menu-item {
  color: #000000;
  display: flex;
  background-color: #fff;
  margin: 5rpx 0rpx;
  /* flex-direction: column; */
  padding: 10rpx;
  font-size: 32rpx;
  align-items: center;
}
.menu-item-content{
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.member_icon{
  width: 40rpx;
  height: 40rpx;
  border-radius: 32rpx;
}

.mini-btn{
  margin-right: 10rpx;
}