.menu-list {
  display: flex;
  flex-direction: column;
  background-color: #fbf9fe;
  margin-top: 5rpx;
}
.menu-item {
  color: #000000;
  display: flex;
  background-color: #fff;
  margin: 5rpx 0rpx;
  flex-direction: column;
}
.menu-item-main {
  display: flex;
  padding: 20rpx;
  border-radius: 5rpx;
  align-items: center;
  font-size: 32rpx;
  justify-content: space-between;
}
.menu-item-arrow {
  width: 32rpx;
  height: 32rpx;
  transition: 400ms;
}

.banner{
  width:100%;
  height:234rpx;
}
.banner swiper{
  height:100%;
}
.slide-image{
  display:block;
  width:100%;
  height:100%;
}