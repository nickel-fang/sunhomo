.container{
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
}

.menu-list {
  color:#000;
  margin-top: 5rpx;
}

.menu-item-main {
  margin: 10rpx 20rpx;
  padding: 20rpx 30rpx;
  border-radius: 5rpx;
  font-size: 28rpx;
  box-shadow: 1rpx 4rpx 4rpx rgba(0,0,0,.1);
  background:#fff;
}

.menu-item {
  color: #000000;
  margin: 5rpx 0rpx;
}
.menu-header{
  display: flex;
  justify-content: space-between;
  font-weight: bold;
  font-size:32rpx;
}
.header-right button{
  margin-left:20rpx;
}
page{
  background:#f1f1f1;
  min-height: 100%;
}
.icon-vs{
  width:18%;
  height:100rpx;
}
.battle-box{
  display: flex;
  align-items: center;
  margin-top:20rpx;
}
.battle-player{
  width:40%;
  text-align: center;
  line-height: 1.5;
  font-weight: bold;
  font-size:28rpx;
  position: relative;
}
.icon-win{
  position: absolute;
  top:0rpx;

  width:70rpx;
  height:70rpx;
}
.icon-win.left{
  left:0rpx;
}
.icon-win.right{
  right:0rpx;
}
.battle-player view{
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.call-wrap{
  margin-top:20rpx;
}
.call-line{
  height: 30rpx;
  background:#fae706 ;
  border-radius: 15rpx;
  position: relative;
  overflow: hidden;
}
.line-cover{
  height: 30rpx;
  background:#ff7f06 ;
  position: absolute;
  top:0;
  left:0;
  width:50%;
}
.call-main{
  display: flex;
  margin-top:30rpx;
}
.call-box{
  width:50%;
}
.call-btn{
  width:50%;
  text-align: center;
}
.call-list{
  min-height:50rpx;
  padding:10rpx;
  overflow: hidden;
}
.icon-photo{
  width:40rpx;
  height:40rpx;
  border-radius: 20rpx;
  background:green;
  margin:5rpx;
}
.list-left .icon-photo{
  float: left;
}
.list-right .icon-photo{
  float: right;
}
.call-btn-box{
  margin-top: 5rpx;
  display: flex;
}
.number-left{
  position: absolute;
  top:50%;
  transform:translateY(-50%);
  left:20rpx;
}
.number-right{
  position: absolute;
  top:50%;
  transform:translateY(-50%);
  right:20rpx;
}