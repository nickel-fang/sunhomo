.container{
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
}

.space {
  /* width: 100vw; */
  height: 16rpx;
  background: #F4F5F9;
}
.line {
  width:718px;
  height:1px;
  background:#EEEEEE;
  margin-left: 32rpx;
}
.menu-item {
  /* width: 100vw; */
  /* height: 104rpx; */
  padding: 0 32rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.menu-item .title {
  font-size:30rpx;
  color:#333;
  line-height:104rpx;
  padding-left: 5rpx;
  font-weight: bold;
}
.menu-item .l {
  font-size:30rpx;
  color:#333;
  line-height:60rpx;
  padding-left: 5rpx;
}
.menu-item .r {
  font-size:32rpx;
  color:#333;
  line-height:104rpx;
  padding-right:5rpx;
  text-align: right;
}
.menu-item .next {
  width: 40rpx;
  height: 40rpx;
}
.menu-item button {
  margin: 0;
}

.member_icon{
  width: 40rpx;
  height: 40rpx;
  border-radius: 32rpx;
}